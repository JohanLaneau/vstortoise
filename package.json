{
    "name": "vstortoise",
    "displayName": "TortoiseSVN",
    "description": "A simple TortoiseSVN binding fro Visual Code",
    "repository": {
        "url": "https://github.com/florentulve/vstortoise"
    },
    "version": "1.3.0",
    "publisher": "florentulve",
    "engines": {
        "vscode": "^1.72.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.svn",
        "onCommand:vstortoise.svnLog",
        "onCommand:vstortoise.svnRevert",
        "onCommand:vstortoise.svnCommit",
        "onCommand:vstortoise.svnUpdate",
        "onCommand:vstortoise.svnDiff",
        "onCommand:vstortoise.svnBlame"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "TortoiseSVN configuration",
            "properties": {
                "TortoiseSVN.path": {
                    "type": "string",
                    "default": "C:\\Program Files\\TortoiseSVN",
                    "description": "Speficies the path to TortoiseSVN installation"
                }
            }
        },
        "commands": [
            {
                "command": "vstortoise.svnLog",
                "title": "SVN Log"
            },
            {
                "command": "vstortoise.svnRevert",
                "title": "SVN Revert"
            },
            {
                "command": "vstortoise.svnCommit",
                "title": "SVN Commit"
            },
            {
                "command": "vstortoise.svnUpdate",
                "title": "SVN Update"
            },
            {
                "command": "vstortoise.svnDiff",
                "title": "SVN Diff"
            },
            {
                "command": "vstortoise.svnBlame",
                "title": "SVN Blame"
            }
        ],
        "keybindings": [
            {
                "command": "vstortoise.svnLog",
                "key": "shift+alt+l"
            },
            {
                "command": "vstortoise.svnCommit",
                "key": "shift+alt+c"
            },
            {
                "command": "vstortoise.svnDiff",
                "key": "shift+alt+d"
            },
            {
                "command": "vstortoise.svnRevert",
                "key": "shift+alt+r"
            },
            {
                "command": "vstortoise.svnUpdate",
                "key": "shift+alt+u"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "vstortoise.svnUpdate",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnCommit",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnLog",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnRevert",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnDiff",
                    "group": "TortoiseSVN"
                }
            ],
            "editor/context": [
                {
                    "command": "vstortoise.svnUpdate",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnCommit",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnLog",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnRevert",
                    "group": "TortoiseSVN"
                },
                {
                    "command": "vstortoise.svnDiff",
                    "group": "TortoiseSVN"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "command-exists": "^1.2.9"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.72.0",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.24.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.8.4"
    }
}
