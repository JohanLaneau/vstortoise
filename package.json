{
    "name": "vstortoise",
    "displayName": "TortoiseSVN",
    "description": "A simple TortoiseSVN binding from Visual Code",
    "repository": {
        "url": "https://github.com/florentulve/vstortoise"
    },
    "version": "1.3.1",
    "publisher": "florentulve",
    "engines": {
        "vscode": "^1.72.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.svn",
        "onCommand:vstortoise.svnLog",
        "onCommand:vstortoise.svnRevert",
        "onCommand:vstortoise.svnCommit",
        "onCommand:vstortoise.svnUpdate",
        "onCommand:vstortoise.svnDiff",
        "onCommand:vstortoise.svnBlame",
        "onCommand:vstortoise.svnProperties",
        "onCommand:vstortoise.svnRepobrowser"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "TortoiseSVN configuration",
            "properties": {
                "TortoiseSVN.path": {
                    "type": "string",
                    "default": "C:\\Program Files\\TortoiseSVN",
                    "description": "Speficies the path to TortoiseSVN installation"
                }
            }
        },
        "commands": [
            {
                "command": "vstortoise.svnLog",
                "title": "Log"
            },
            {
                "command": "vstortoise.svnRevert",
                "title": "Revert"
            },
            {
                "command": "vstortoise.svnCommit",
                "title": "Commit"
            },
            {
                "command": "vstortoise.svnUpdate",
                "title": "Update"
            },
            {
                "command": "vstortoise.svnDiff",
                "title": "Diff"
            },
            {
                "command": "vstortoise.svnBlame",
                "title": "Blame"
            },
            {
                "command": "vstortoise.svnProperties",
                "title": "Properties"
            },
            {
                "command": "vstortoise.svnRepobrowser",
                "title": "Repo-browser"
            }
        ],
        "keybindings": [
            {
                "command": "vstortoise.svnLog",
                "key": "shift+alt+l"
            },
            {
                "command": "vstortoise.svnCommit",
                "key": "shift+alt+c"
            },
            {
                "command": "vstortoise.svnDiff",
                "key": "shift+alt+d"
            },
            {
                "command": "vstortoise.svnRevert",
                "key": "shift+alt+r"
            },
            {
                "command": "vstortoise.svnUpdate",
                "key": "shift+alt+u"
            }
        ],
        "submenus": [
            {
                "id": "tortoise",
                "label": "TortoiseSVN"
            }
        ],
        "menus": {
            "tortoise": [
                {
                    "command": "vstortoise.svnLog",
                    "group": "TortoiseSVN@1"
                },
                {
                    "command": "vstortoise.svnUpdate",
                    "group": "TortoiseSVN@2"
                },
                {
                    "command": "vstortoise.svnCommit",
                    "group": "TortoiseSVN@3"
                },
                {
                    "command": "vstortoise.svnRevert",
                    "group": "TortoiseSVN@4"
                },
                {
                    "command": "vstortoise.svnDiff",
                    "group": "TortoiseSVN@5"
                },
                {
                    "command": "vstortoise.svnBlame",
                    "group": "TortoiseSVN@6"
                },
                {
                    "command": "vstortoise.svnProperties",
                    "group": "TortoiseSVN@7"
                },
                {
                    "command": "vstortoise.svnRepobrowser",
                    "group": "TortoiseSVN@8"
                }
            ],
            "explorer/context": [
                {
                    "submenu": "tortoise",
                    "group": "TortoiseSVN"
                }
            ],
            "editor/context": [
                {
                    "submenu": "tortoise",
                    "group": "TortoiseSVN"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "command-exists": "^1.2.9"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "^22.5.0",
        "@types/vscode": "^1.72.0",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.24.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.9.5"
    }
}
